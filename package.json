    "@nuxtjs/dotenv": "^1.1.1",
    "express": "^4.15.3",
    "nuxt": "^1.0.0",
    "nuxt-buefy": "^0.0.4",
    "nuxt-fontawesome": "^0.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.5",
